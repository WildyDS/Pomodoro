{
  "name": "Pomodoro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --testMatch='**/__tests__/**/*.test.js' --setupFiles=./__tests__/Setup",
    "lint": "eslint .",
    "precommit": "yarn tsc && yarn lint && yarn test",
    "android:emulator": "(emulator=$($ANDROID_HOME/tools/bin/./avdmanager list avd | grep Name:| head -n 1 | awk '{print $2}') && $ANDROID_HOME/tools/./emulator -avd \"$emulator\") | (sleep 15 && adb reverse tcp:9090 tcp:9090 && echo connected)"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "eslint-plugin-lodash": "^6.0.0",
    "lodash": "^4.17.15",
    "react": "16.9.0",
    "react-native": "0.62.3",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.1.1",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.37",
    "@types/react-native": "^0.66.9",
    "@types/react-redux": "^7.1.20",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/seamless-immutable": "^7.1.16",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "husky": "^3.1.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.61.0",
    "react-test-renderer": "17.0.2",
    "reactotron-apisauce": "^3.0.0",
    "reactotron-react-native": "^4.0.2",
    "reactotron-redux": "^3.1.2",
    "reactotron-redux-saga": "^4.2.2",
    "typescript": "^4.5.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
